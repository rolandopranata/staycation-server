<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary">DataTables Booking</h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>No</th>
            <th>Invoice</th>
            <th>Date</th>
            <th>Title</th>
            <th>Member Name</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% booking.forEach((data, i) => { %>
            <tr>
              <td>
                <%= i + 1 %>
              </td>
              <td>
                #
                <%= data.invoice %>
              </td>
              <td>
                <%= data.bookingStartDate.toLocaleString().split(',')[0] %> s/d
                  <%= data.bookingEndDate.toLocaleString().split(',')[0] %>
              </td>
              <td>
                <%= data.itemId.title %>
              </td>
              <td>
                <%= data.memberId.firstName %>
                  <%= data.memberId.lastName %>
              </td>
              <td>
                <%= data.payments.status %>
              </td>
              <td class="d-flex justify-content-center align-items-center">
                <a href="/admin/booking/<%= booking[i].id %> " class="btn btn-success btn-circle btn-sm button-update">
                  <i class="fas fa-eye"></i>
                </a>
              </td>
            </tr>
            <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>